{
  "name": "react-redux-antd-ts",
  "version": "0.0.1",
  "description": "基于react+redux+antd框架",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "devc": "better-npm-run devc",
    "qa": "better-npm-run qa",
    "prod": "better-npm-run prod",
    "start": "better-npm-run dev",
    "dll": "node webpack/dll.config",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "devc": {
      "command": "npm run clean && node bin/compile",
      "env": {
        "NODE_ENV": "devc",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run clean  &&  better-npm-run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "qa": {
      "command": "npm run clean && node bin/compile",
      "env": {
        "NODE_ENV": "qa",
        "DEBUG": "app:*"
      }
    },
    "prod": {
      "command": "npm run clean && node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xwatson/react-redux-antd-ts.git"
  },
  "author": "Xwatson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Xwatson/react-redux-antd-ts/issues"
  },
  "homepage": "https://github.com/Xwatson/react-redux-antd-ts#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.2",
    "better-npm-run": "^0.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.11.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "typescript": "^2.4.2",
    "webpack": "^3.5.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "@types/react": "^16.0.2",
    "@types/react-dom": "^15.5.2",
    "@types/react-intl": "^2.3.1",
    "@types/react-redux": "^5.0.0",
    "@types/react-router": "^4.0.14",
    "@types/react-router-redux": "^5.0.5",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "antd": "^2.12.6",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "ts-loader": "^2.3.2"
  }
}
